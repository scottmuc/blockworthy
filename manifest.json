{
  "manifest_version": 2,

  "name": "BlockWorthy",
  "description": "This extension blocks clickbait",
  "version": "0.0.3",

  "permissions": ["storage"],

  "options_page": "options.html",

  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
  }]
}

